@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Bootstrap, from Twitter</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Le styles -->
    <link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/bootstrap-responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/dropdowns.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/docs.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet">
    @this.RenderSection("Styles", false)
</head>
<body>
    @this.RenderSection("NavigationBar", false)
    @this.RenderSection("SearchBar", false)
    @this.RenderBody()
    
    <script src="@Url.Content("~/Scripts/jquery-2.0.2.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-affix.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-alert.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-button.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-popover.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-tooltip.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-transition.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-typeahead.js")"></script>
    <script src="@Url.Content("~/Scripts/dropdown.js")"></script>
    <script language="javascript">
        $(document).ready(function() {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
    @this.RenderSection("Scripts", false)
</body>
</html>
